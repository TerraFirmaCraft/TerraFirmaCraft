{
	"multipart": [
		{ "when": { "facing": "up" },
			"apply": [
				{ "model": "tfc:plants/licorice_fern" }
			]
		},
		{ "when": { "facing": "down" },
			"apply": [
				{ "model": "tfc:plants/licorice_fern", "x": 180 }
			]
		},
		{ "when": { "facing": "east" },
			"apply": [
				{ "model": "tfc:plants/licorice_fern", "x": 90, "y": 90 }
			]
		},
		{ "when": { "facing": "south" },
			"apply": [
				{ "model": "tfc:plants/licorice_fern", "x": 90, "y": 180 }
			]
		},
		{ "when": { "facing": "west" },
			"apply": [
				{ "model": "tfc:plants/licorice_fern", "x": 90, "y": 270 }
			]
		},
		{ "when": { "facing": "north" },
			"apply": [
				{ "model": "tfc:plants/licorice_fern", "x": 90 }
			]
		}
	]
}
