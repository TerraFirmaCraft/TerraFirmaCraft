{
   "multipart": [
       {   "when": { "part": "single", "age": "0|1|2|3|4|5|6|7" },
           "apply": { "model": "tfc:plants/tall_fescue_grass_shortest" }
       },
       {   "when": { "part": "single", "age": "8|9|10|11|12|13|14|15" },
           "apply": { "model": "tfc:plants/tall_fescue_grass_shorter" }
       },
       {   "when": { "part": "upper", "age": "0|1|2|3|4|5|6|7" },
           "apply": { "model": "tfc:plants/tall_fescue_grass_short" }
       },
       {   "when": { "part": "upper", "age": "8|9|10|11|12|13|14|15" },
           "apply": { "model": "tfc:plants/tall_fescue_grass" }
       }
   ]
}