{
   "multipart": [
       {   "when": { "down": "true" },
           "apply": { "model": "tfc:plants/rattan" }
       },
       {   "when": { "east": "true" },
           "apply": { "model": "tfc:plants/rattan", "x": 270, "y": 90 }
       },
       {   "when": { "north": "true" },
           "apply": { "model": "tfc:plants/rattan", "x": 270 }
       },
       {   "when": { "south": "true" },
           "apply": { "model": "tfc:plants/rattan", "x": 270, "y": 180 }
       },
       {   "when": { "up": "true" },
           "apply": { "model": "tfc:plants/rattan", "x": 180 }
       },
       {   "when": { "west": "true" },
           "apply": { "model": "tfc:plants/rattan", "x": 270, "y": 270 }
       }
   ]
}