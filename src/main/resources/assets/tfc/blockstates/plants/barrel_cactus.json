{
	"multipart": [
		{ "when": { "age": "0", "part": "single" },
			"apply": [
				{ "model": "tfc:plants/barrel_cactus_small" }
			]
		},
		{ "when": { "part": "lower" },
			"apply": [
				{ "model": "tfc:plants/barrel_cactus_lower" }
			]
		},
		{ "when": { "part": "middle" },
			"apply": [
				{ "model": "tfc:plants/barrel_cactus_middle" }
			]
		},
		{ "when": { "age": "1|2|3", "part": "single" },
			"apply": [
				{ "model": "tfc:plants/barrel_cactus_single" }
			]
		},
		{ "when": { "part": "upper" },
			"apply": [
				{ "model": "tfc:plants/barrel_cactus_upper" }
			]
		},
		{ "when": { "part": "upper", "stage": "1" },
			"apply": [
				{ "model": "tfc:plants/barrel_cactus_flower_0" }
			]
		},
		{ "when": { "age": "1", "part": "single" , "stage": "2" },
			"apply": [
				{ "model": "tfc:plants/barrel_cactus_flower_1" }
			]
		},
		{ "when": { "age": "0|1", "part": "upper" , "stage": "2" },
			"apply": [
				{ "model": "tfc:plants/barrel_cactus_flower_1" }
			]
		},
		{ "when": { "age": "2|3", "part": "single|upper" , "stage": "2" },
			"apply": [
				{ "model": "tfc:plants/barrel_cactus_flower_2" }
			]
		},
		{ "when": { "age": "1|2|3", "part": "single" , "stage": "3" },
			"apply": [
				{ "model": "tfc:plants/barrel_cactus_flower_3" }
			]
		},
		{ "when": { "part": "upper" , "stage": "3" },
			"apply": [
				{ "model": "tfc:plants/barrel_cactus_flower_3" }
			]
		}
	]
}
