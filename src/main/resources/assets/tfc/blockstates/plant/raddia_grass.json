{
  "multipart": [
    {
      "when": {
        "age": "0"
      },
      "apply": {
        "model": "tfc:block/plant/raddia_grass_shortest"
      }
    },
    {
      "when": {
        "age": "1"
      },
      "apply": {
        "model": "tfc:block/plant/raddia_grass_shorter"
      }
    },
    {
      "when": {
        "age": "2"
      },
      "apply": {
        "model": "tfc:block/plant/raddia_grass_short"
      }
    },
    {
      "when": {
        "age": "3"
      },
      "apply": {
        "model": "tfc:block/plant/raddia_grass"
      }
    }
  ]
}